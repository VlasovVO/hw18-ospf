hostname Router
log file /var/log/quagga/quagga.log
hostname ospfd
log stdout
!
password zebra
enable password zebra
!
interface eth0
 ipv6 nd suppress-ra
!
interface eth1
 ip address 192.168.50.10/24
 ipv6 nd suppress-ra
!
interface eth2
 ip address 192.168.50.20/24
 ipv6 nd suppress-ra
!
interface lo
!
router ospf
 ospf router-id 0.0.0.2
 network 192.168.50.0/24 area 0.0.0.0
!
ip forwarding
!
line vty
!
